{% extends ":Front:front_layout.html.twig" %}

    {% block content %}
        {% block core_content %}
            <div class="row">
                <div class="col-sm-4 col-sm-offset-4">

                    <div class="page-title">
                        <h1>Confirm</h1>
                    </div><!-- /.page-title -->

                    {{ dump(tempUser) }}

                    {#Form Start#}
                    {{ form_start(form)}}
