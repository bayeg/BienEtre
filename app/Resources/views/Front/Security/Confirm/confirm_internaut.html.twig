                        {#{{ form_widget(form.type, {'attr':{'class':'form-control'}}) }}#}
                    {#</div><!-- /.form-group -->#}




                    {{ form_rest(form) }}

                    {#Submit#}
                    {{ form_widget(form.submit,{'label':'Register','attr':{'class':'btn btn-primary pull-right'}}) }}

